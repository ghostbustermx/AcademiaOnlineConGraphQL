  "dependencies": {
    "@graphql-tools/utils": "^10.1.2",
    "apollo-server-express": "^3.13.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "graphql-import-node": "^0.0.5",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^9.0.1",
